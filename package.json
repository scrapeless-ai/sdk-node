{
  "name": "@scrapeless-ai/sdk",
  "version": "1.5.1",
  "description": "Node SDK for Scrapeless AI",
  "author": "Scrapeless AI",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "build": "tsdown --clean --platform node",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "husky",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major",
    "release:patch": "standard-version --release-as patch",
    "release:prerelease": "standard-version --prerelease",
    "release:alpha": "standard-version --prerelease alpha",
    "release:beta": "standard-version --prerelease beta",
    "release:dry-run": "standard-version --dry-run"
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "keywords": [
    "scrapeless",
    "scraping",
    "browser",
    "automation",
    "webscraping",
    "webcrawling",
    "captcha",
    "serp",
    "proxy"
  ],
  "dependencies": {
    "dotenv": "^16.5.0",
    "form-data": "^4.0.2",
    "node-fetch": "2.7.0",
    "playwright-core": "^1.52.0",
    "puppeteer-core": "^24.9.0",
    "stack-trace": "1.0.0-pre2",
    "tsdown": "^0.11.13",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "winston-transport": "^4.9.0",
    "zod": "^3.25.42",
    "zod-to-json-schema": "^3.24.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.27.0",
    "@jest/globals": "^29.7.0",
    "@types/node": "^22.15.27",
    "@types/node-fetch": "^2.6.12",
    "@typescript-eslint/eslint-plugin": "^8.33.0",
    "@typescript-eslint/parser": "^8.33.0",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^16.1.0",
    "prettier": "^3.5.3",
    "standard-version": "^9.5.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vitest": "^3.1.4"
  },
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "default": "./dist/index.js"
    }
  },
  "jest": {
    "testMatch": [
      "**/*.test.mjs"
    ],
    "transform": {},
    "extensionsToTreatAsEsm": [
      ".mjs"
    ],
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scrapeless-ai/sdk-node.git"
  },
  "bugs": {
    "url": "https://github.com/scrapeless-ai/sdk-node/issues"
  },
  "homepage": "https://github.com/scrapeless-ai/sdk-node#readme",
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix --quiet",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  }
}
